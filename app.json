{
  "name": "servant-elm-poc",
  "description": "boilerplate for running servant + elm on heroku",
  "logo": "",
  "repository": "",
  "buildpacks": [
    {
      "url": "https://github.com/mietek/haskell-on-heroku"
    },
    {
      "url": "https://github.com/srid/heroku-buildpack-elm.git"
    },
    {
      "url": "https://github.com/mfine/heroku-buildpack-stack"
    }
  ],
  "env": {
    "ELM_COMPILE": "make clean elm",
    "BUILDPACK_URL": {
      "description": "Heroku buildpack for deploying Haskell apps",
      "value": "https://github.com/mietek/haskell-on-heroku"
    },
    "TUTORIAL_HOME": {
      "description": "Welcome message",
      "required": false,
      "value": "Welcome to Haskell on Heroku"
    }
  }
}
